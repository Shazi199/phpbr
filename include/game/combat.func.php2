function check_gender($nm_a, $nm_d, $gd_a, $gd_d, $a_ky) {
	global $log;
	$gd_dmg_p = 1;
	
	if ( ( ( ( isset ( $a_ky['sl'] ) ) && ( $gd_a != $gd_d ) ) || ( ( isset($a_ky['sg']) ) && ($gd_a == $gd_d ) ) ) && (! rand ( 0, 4 ))) {
		
		$log .= "<span class=\"red\">{$nm_a}被{$nm_d}迷惑，无法全力攻击！</span>";
