function set_orig_dmg ($adata,$ddata,$att,$def,$ws,$wp_kind,$active = 0){
	global $skill_dmg, $dmg_fluc, $weather;
	$pls = $adata['pls'];
	$attack_p = get_attack_p ( $weather, $pls, $adata['pose'], $adata['tactic'], $adata['club'], $adata['inf'], $active );
	$att_pow = $att * $attack_p;
	$defend_p = get_defend_p ( $weather, $pls, $ddata['pose'], $ddata['tactic'], $ddata['club'], $ddata['inf'], 1-$active );
	$def_pow = $def * $defend_p;
	$damage = ($att_pow / $def_pow) * $ws * $skill_dmg [$wp_kind];
	$dmg_factor = (100 + rand ( - $dmg_fluc [$wp_kind], $dmg_fluc [$wp_kind] )) / 100;
	$damage = round ( $damage * $dmg_factor * rand ( 4, 10 ) / 10 );
	return $damage;
}

function get_pos_words($data,$htime,$active = 0){
	global $log,$infinfo;
	if($htime['hit']<=0){return;}
	if(!$active){$name = '你';}
	else{$name = $data['name'];}
	if($htime['hit']>1){
		$inflistwords = '';
		$infliststr = implode('',$htime['hitlist']);
		foreach(Array('h','b','a','f') as $val){
			if(in_array($val,$htime['hitlist'])){
				$infliststr = str_replace($val,'',$infliststr);
				if(empty($inflistwords)){
					$inflistwords = $infinfo[$val].'部';
					
				}elseif(!empty($infliststr)){
					$inflistwords .= '、'.$infinfo[$val].'部';
					
				}else{
					$inflistwords .= '和'.$infinfo[$val].'部';
				}
			}
		}
		
	}else{
		$inflistwords = $infinfo[$htime['hitlist'][0]].'部';
	}
	$log .= "攻击命中了{$name}的{$inflistwords}！<br>";
	return;
}

function get_multi_pos_dmg ($data, & $dmg, $htime ,$active = 0){
	global $log,$pos_dmg_fac;
	if($htime['hit']<=0){return;}
	$odmg = $dmg;
	$dmg_p_up = 1;
	$dmg_p = 1;
	if($htime['hit']>1){
		foreach($htime['hitlist'] as $val){
			$dmg_p += $dmg_p_up * $pos_dmg_fac[$val];
			$dmg_p_up = $dmg_p_up <= 0.5 ? $dmg_p_up : $dmg_p_up - 0.1;
		}
		$dmg = ceil($dmg * $dmg_p);


		$log .= "造成了{$odmg}×{$dmg_p}=<span class=\"red\">$dmg</span>点伤害！<br>";
	}else{
		$dmg_p = $pos_dmg_fac[$htime['hitlist'][0]];
		$dmg = ceil($dmg * $dmg_p);
		$log .= "造成了<span class=\"red\">$dmg</span>点伤害！<br>";
	}
	
	return;
}

