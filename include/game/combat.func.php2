	} elseif ( ( ( ( isset($a_ky['sl']) ) && ($gd_a == $gd_d)) || ((isset($a_ky['sg'])) && ($gd_a != $gd_d))) && (! rand ( 0, 4 ))) {
		
		$log .= "<span class=\"red\">{$nm_a}被{$nm_d}激怒，伤害加倍！</span>";
		$gd_dmg_p = 2;
	}
	return $gd_dmg_p;
}

