//							}else{
//								naddnews($now,'inf',$name,$w_name,$ex_inf_sign);
//							}	
						}
					}
					
					$log .= '<br />';
					
				} else { //属性防御
					$log .= "{$dmgnm}被防具抵消了！";
					
					if($dmgf == 0){
