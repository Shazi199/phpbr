function get_dmg($adata,$ddata,$wep_kind = 'N',$active = 0){ //计算伤害并生成作战过程
	global $now,$nosta,$infobbs, $infinfo, $attinfo, $skillinfo,  $wepimprate;
	global $log,$message;
	
	extract($adata, EXTR_PREFIX_ALL | EXTR_REFS, 'a' );
	extract($ddata, EXTR_PREFIX_ALL | EXTR_REFS, 'd' );
	if($active){
		$a_sh_name = '你';$d_sh_name = $d_name;
	}else{
		$a_sh_name = $a_name;$d_sh_name = '你';
	}
	$is_wpg = false;//判断是否枪械用作钝器，并计算武器攻击力
	if ((strpos ( $a_wepk, 'G' ) == 1) && ($a_weps == $nosta)) {
		if (($wep_kind == 'G') || ($wep_kind == 'P')) {
			$wep_kind = 'P';
			$is_wpg = true;
			$watt = round ( $wepe / 5 );
		} else {
			$watt = $a_wepe;
		}
	} elseif ($wep_kind == 'N') {
		$watt =  round (${$skillinfo [$wep_kind]}*2/3);
	} else {
		$watt = $a_wepe * 2;
	}
	$log .= "{$a_sh_name}使用{$a_wep}<span class=\"yellow\">$attinfo[$wep_kind]</span>{$d_sh_name}！<br>";
	$att_key = player_property($adata,'att');
	$def_key = player_property($ddata,'def');
	$wep_skill = & ${'a_'.$skillinfo [$wep_kind]};
	$hitrate = get_hitrate ( $wep_kind, $wep_skill, $a_inf, $a_pls );
	if($active){
		$damage_p = set_dmg_p($adata, $att_key, $message ,1);
	}else{
		$damage_p = set_dmg_p($adata, $att_key);
	}
	$hit_time = get_hit_time($a_wepk, $a_weps, $wep_kind, $wep_skill, $hitrate, $is_wpg);
	if ($hit_time ['hit'] > 0) {//判断是否命中
		//暂时剥离性别判断系统
		$attack = $a_att + $watt;//基础伤害
		$defend = $d_def + $d_arbe + $d_arhe + $d_arae + $d_arfe;
		$damage = set_orig_dmg ($adata,$ddata, $attack, $defend, $wep_skill, $wep_kind ,$active);
		if ($wep_kind == 'F') {
			$damage = round ( ($a_wepe + $damage) * get_WF_p($a_club, $a_wepe, $active) );
		}
		
		$damage *= checkarb ( $wep_kind, $def_key );
		$damage *= $damage_p;
		//$damage *= $gender_dmg_p;
		
		$damage = get_ex_dmg($damage, $adata, $ddata, $att_key, $def_key, $wep_kind, $wep_skill, $active);
		$damage = $damage > 1 ? round ( $damage ) : 1;
		get_pos_words($ddata, $hit_time,$active);
		get_multi_pos_dmg ($ddata,$damage, $hit_time,$active);
		checkdmg ( $a_name, $d_name, $damage );
		get_dmg_punish($damage, $adata, $att_key, $active);
		set_inf($ddata,$hit_time['inf'],$wep_kind,$active);
		
		check_KP_wep($adata, $hit_time ['imp'], $active);
		set_exprgup($adata,$ddata,$active);
		
	} else {
		$damage = 0;
		$log .= "但是没有击中！<br>";
	}
	check_GCDF_wep($adata, $hit_time ['atk'], $wep_kind,$active);
	addnoise ( $wep_kind, $a_wepsk, $now, $a_pls, $a_pid, $d_pid);
	if($a_club == 10){
		$wep_skill +=2;
	}else{
		$wep_skill +=1;
	}
	return $damage;
}

