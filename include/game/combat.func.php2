function get_hit_time($wepk, $weps, $wep_kind, $wep_skill, $htr, $is_wpg) {
	global $log, $nosta, $infobbs, $wepimprate;
	$atime = get_wephittimes($wepk,$is_wpg);
	//$atime = is_numeric(substr($wepk,3,1)) ? substr($wepk,3,1) : 1;
	$infr = $infobbs[$wep_kind];
	$wimpr = $wepimprate[$wep_kind];
	$inffields = Array('h','b','a','f');
	if ($weps == $nosta) {
		$wimpr *= 2;
		if ($is_wpg) {
			$wimpr *= 4;
		}
	}
	if ($atime>1) {
		$batime = $wep_skill >= 100*($atime-1) ? $atime : 1+ floor($wep_skill/100);
		$atime = rand($batime,$atime);
		//$atk_t = $ws >= 800 ? 6 : 2 + floor ( $ws / 200 );
		if (($wep_kind == 'G' || $wep_kind == 'C' || $wep_kind == 'D' || $wep_kind == 'F') && $weps!==$nosta && $atime > $weps) {
			$atime = $weps;
		}
		
		$hitlist = Array();
		$inflist = Array();
		$wimptime = 0;
		//if($htr>100){$htr=100;}
		for($i = 1; $i <= $atime; $i ++) {
			$dice = rand ( 0, 99 );
			$dice2 = rand ( 0, 99 );
			$dice3 = rand ( 0, 99 );//echo $htr.' ';
			if ($dice < $htr) {
				$hitpos = $inffields[rand(0,3)];
				$hitlist[] = $hitpos;
				if ($dice2 < $infr && !in_array($hitpos,$inflist)) {
					$inflist[] = $hitpos;
					//$inftime ++;
				}
				if ($dice3 < $wimpr) {
					$wimptime ++;
				}
			}
			$htr *= $htr > 50 ? 0.9 : 0.95;
			$infr *= 0.9;
			$wimpr *= $wimpr <= 0 ? 1 : 1.2;
		}
	} else {
		$atime = 1;
		$hitlist = Array();
		$inflist = Array();
		$wimptime = 0;
		$dice = rand ( 0, 99 );
		$dice2 = rand ( 0, 99 );
		$dice3 = rand ( 0, 99 );
		if ($dice < $htr) {
			$hitpos = $inffields[rand(0,3)];
			$hitlist = Array($hitpos);
			if ($dice2 < $infr && !in_array($hitpos,$inflist)) {
				$inflist = Array($hitpos);
			}
			if ($dice3 < $wimpr) {
				$wimptime = 1;
			}
		}
	}
	$htime = count($hitlist);
	$itime = count($inflist);
	if ($atime > 1 && $htime > 0) {
		$log .= "{$atime}次连续攻击命中<span class=\"yellow\">{$htime}</span>次！";
	}	
	return Array ('atk' => $atime, 'hit'=> $htime, 'hitlist' => $hitlist, 'inf' => $itime ,'imp' => $wimptime );
}

